{
    "name": "stefanius/laravel-fixtures",
    "description": "Load YAML fixtures in your Laravel project",
    "keywords": ["fixtures", "laravel", "yaml", "yml", "database", "seed", "seeder", "migration"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.2.*|5.3.*",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.3",
        "phpspec/phpspec": "~2.1",
        "laravel/homestead": "^3.0",
        "symfony/dom-crawler": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Stefanius\\LaravelFixtures\\": "src/Stefanius/LaravelFixtures/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests",
            "testdata"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
